<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TextStyle1">
        <item name="android:layout_margin">@dimen/text_margin</item>
        <item name="android:fontFamily">@font/butcherman</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">@dimen/text_margin</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">#090909</item>
        <item name="android:textSize">@dimen/text_sizze</item>
    </style>

</resources>
